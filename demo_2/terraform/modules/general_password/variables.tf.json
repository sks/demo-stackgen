{
    "variable": {
        "password_length": [
            {
                "description": "Length of the generated password",
                "type": "number",
                "default": 16
            }
        ],
        "include_special": [
            {
                "description": "Include special characters",
                "type": "bool",
                "default": true
            }
        ],
        "include_uppercase": [
            {
                "description": "Include uppercase letters",
                "type": "bool",
                "default": true
            }
        ],
        "include_lowercase": [
            {
                "description": "Include lowercase letters",
                "type": "bool",
                "default": true
            }
        ],
        "include_numbers": [
            {
                "description": "Include numeric characters",
                "type": "bool",
                "default": true
            }
        ],
        "min_uppercase": [
            {
                "description": "Minimum number of uppercase letters",
                "type": "number",
                "default": 0
            }
        ],
        "min_lowercase": [
            {
                "description": "Minimum number of lowercase letters",
                "type": "number",
                "default": 0
            }
        ],
        "min_special": [
            {
                "description": "Minimum number of special characters",
                "type": "number",
                "default": 0
            }
        ],
        "min_numeric": [
            {
                "description": "Minimum number of numeric characters",
                "type": "number",
                "default": 0
            }
        ],
        "override_special": [
            {
                "type": "string",
                "description": "Override the default special character set",
                "default": "!#$%&*()-_=+[]{}<>:?"
            }
        ]
    }
}